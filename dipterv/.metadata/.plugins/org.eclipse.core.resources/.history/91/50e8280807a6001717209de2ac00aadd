package com.sample
 
import com.sample.DroolsTest.TimeSerie;
 
rule "TS"
    when
        ts : TimeSerie()
    then
        System.out.println( ts.timeSerieData );
        ts.setMessage( "Goodbye cruel world" );
        update( ts );
end

