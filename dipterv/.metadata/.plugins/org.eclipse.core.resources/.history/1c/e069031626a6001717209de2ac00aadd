package com.sample;

import java.io.FileReader;

import tech.artisanhub.ShapeletClassifier.Classifier;
import tech.artisanhub.ShapeletTrainerMD.DoubleVectorMD;
import weka.core.Instance;
import weka.core.SparseInstance;

public class TimeSerieClassifier {
	public boolean isCritical(DoubleVectorMD vector){
		boolean isCrit;
		double[] dArr = new double[vector.getDoubleVector().length];
		for (int x = 0; x < dArr.length; x++){
			dArr[x] = vector.getElement(x).doubleValue();
		}
		Instance instance = new SparseInstance(1.0, vector.getDoubleVector());
		
		Classifier cl = new Classifier();
		
		cl.classifyLine(SparseInstance ts, DroolsTest.shapelet, DroolsTest.cls, DroolsTest.th, DroolsTest.dimension);
		
		return isCrit;
	}
}
