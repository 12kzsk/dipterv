public class SVMClassifier {

	public static int vecSize = 0;
	
	public static boolean isCritialSVM(DoubleVectorMD vector) throws FileNotFoundException, IOException {
		boolean isCrit = false;

		svm_node[] x = new svm_node[vector.getDoubleVector().length];
		for(int j=0;j<m;j++)
		{
			x[j] = new svm_node();
			x[j].index = atoi(st.nextToken());
			x[j].value = atof(st.nextToken());
		}
		
		return isCrit;
	}
}