import weka.core.Instance;

public class TimeSerieClassifier {
	public boolean isCritical(DoubleVectorMD vector){
		boolean isCrit;
		
		Classifier cl = new Classifier();

		cl.classifyLine(Instance ts, DroolsTest.shapelet, DroolsTest.cls, DroolsTest.th, DroolsTest.dimension);
		
		return isCrit;
	}
}
