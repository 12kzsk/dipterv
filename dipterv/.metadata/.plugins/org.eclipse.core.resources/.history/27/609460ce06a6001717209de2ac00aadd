package com.sample
 
import com.sample.DroolsTest.Message;
 
rule "Hello World"
    when
        ts : TimeSerie( currentTs : timeSerieData )
    then
        System.out.println( currentTs );
        ts.setMessage( "Goodbye cruel world" );
        update( ts );
end

